#include <stdio.h>

int main()
{
    int n;
    scanf("%d",&n);
    int a[n],b[n];
    int i;
    
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
     for(i=0;i<n;i++)
    {
        scanf("%d",&b[i]);
    }
    int k,c=0,j;
    scanf("%d",&k);
    
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
    {
        if(a[i]+b[j]==k)
        {
            c++;;}}}
            if(c==0)
            {
                printf("No");
            }
            else
            printf("%d",c);
          

    return 0;
}
